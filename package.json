{
  "name": "spm",
  "version": "1.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cross-env TESTCOVERAGE=true vitest --run",
    "test:ci": "vitest --run",
    "coverage": "cross-env TESTCOVERAGE=true vitest run --coverage",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore --max-warnings 0",
    "fix": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "check": "prettier --check src/**/*.{vue,js}",
    "format": "prettier --write src/**/*.{vue,js}",
    "checkall": "npm run lint && npm run format && npm run test"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.24.18",
    "googleapis": "^144.0.0",
    "pinia": "^2.1.7",
    "prettier": "^3.3.3",
    "uuid": "^10.0.0",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue3-datepicker": "^0.4.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^2.1.3",
    "@vue/compiler-sfc": "^3.5.8",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-vue": "^9.23.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.3.1",
    "vitest": "^2.1.3"
  }
}
